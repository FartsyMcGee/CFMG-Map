        stations.forEach(station => {
            var marker = L.marker([station.lat, station.lon]).addTo(map)
                .bindTooltip(station.call, {permanent: true, direction: 'right', offset: [15, 0], className: 'label'});
            L.circle([station.lat, station.lon], { radius: station.radius * 1000, color: '#ff4500', fillColor: '#ff4500', fillOpacity: 0.1 }).addTo(map);
        });
    </script>
